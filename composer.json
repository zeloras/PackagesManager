{
  "name": "geekcms/packagesmanager",
  "description": "Package system list for geekcms",
  "keywords": [
    "geekcms",
    "geekcms-module",
    "laravel",
    "module"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
    "mockery/mockery": "~1.0",
    "laravel/framework": "5.5.*|5.6.*",
    "paulthebaud/phpunit-generator": "^2.0"
  },
  "authors": [
    {
      "name": "Vitaly Gerasimov",
      "email": "zeloras@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com",
      "homepage": "https://nicolaswidart.com",
      "role": "Developer"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "GeekCms\\PackagesManager\\Providers\\InitServiceProvider"
      ],
      "aliases": {
        "PackageSystem": "GeekCms\\PackagesManager\\Facades\\PackageSystem"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "GeekCms\\PackagesManager\\": "src/"
    },
    "files": [
      "src/loader.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
