{
    "name": "zeloras/geekcms-packages-manager",
    "description": "Package system list for geekcms",
    "keywords": [
        "geekcms",
        "geekcms-module",
        "laravel",
        "module"
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.6",
        "friendsofphp/php-cs-fixer": "^2.7",
        "laravel/framework": "5.6.*",
        "spatie/phpunit-snapshot-assertions": "^1.0",
        "phpstan/phpstan": "^0.9.2",
        "nwidart/laravel-modules": "^3.2"
    },
    "authors": [
        {
            "name": "Vitaly Gerasimov",
            "email": "zeloras@gmail.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "GeekCms\\PackagesManager\\Providers\\PackagesServiceProvider",
                "GeekCms\\PackagesManager\\Providers\\EventServiceProvider"
            ],
            "aliases": {
                "PackageManager": "GeekCms\\PackagesManager\\Facades\\PackageManager"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "GeekCms\\PackagesManager\\": ""
        }
    }
}
